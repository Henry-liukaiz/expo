<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.expo.sso.dao.LoginMapper">
    <resultMap id="BaseResultMap" type="com.expo.entity.domain.User">
        <id column="id" property="id"/>
        <result column="user_name" property="userName"/>
        <result column="vser_name" property="vserName"/>
        <result column="password" property="password"/>
        <result column="email" property="email"/>
        <result column="gender" property="gender"/>
        <result column="status" property="status"/>
        <result column="lastLoginDateTime" property="lastLoginDateTime"/>
        <result column="mobile" property="mobile"/>
        <result column="portrait" property="portrait"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <select id="check" resultMap="BaseResultMap">
        select id,user_name,vser_name from user
    </select>
</mapper>